# vim:ft=plumed

ENVIRONMENTSIMILARITY ...
 SPECIES=257-384
 SIGMA=0.07
 CRYSTAL_STRUCTURE=BCC
 LATTICE_CONSTANTS=0.3407
 LABEL=s
 MEAN
... ENVIRONMENTSIMILARITY

hh: HISTOGRAM ...
   DATA=s
   GRID_MIN=-0.5
   GRID_MAX=2
   GRID_BIN=2000
   BANDWIDTH=0.01
   STRIDE=500
...

DUMPGRID GRID=hh FILE=histo STRIDE=1000

PRINT STRIDE=500  ARG=* FILE=COLVAR
