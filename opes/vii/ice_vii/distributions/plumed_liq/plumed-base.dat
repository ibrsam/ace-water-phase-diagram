# vim:ft=plumed

ENVIRONMENTSIMILARITY ...
 SPECIES=577-864
 SIGMA=replace
 CRYSTAL_STRUCTURE=BCC
 LATTICE_CONSTANTS=0.34072718272556335
 LABEL=s
 MEAN
... ENVIRONMENTSIMILARITY

hh: HISTOGRAM ...
   DATA=s
   GRID_MIN=-0.5
   GRID_MAX=7
   GRID_BIN=2000
   BANDWIDTH=0.01
...

DUMPGRID GRID=hh FILE=histo
PRINT STRIDE=1  ARG=* FILE=COLVAR
